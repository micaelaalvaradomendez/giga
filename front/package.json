		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"svelte": "^5.39.5",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@iconify/svelte": "^5.0.2",
		"@tailwindcss/forms": "^0.5.10",
		"autoprefixer": "^10.4.21",
		"axios": "^1.12.2",
		"lucide-svelte": "^0.545.0",
		"tailwindcss": "^4.1.14"
	}
}
